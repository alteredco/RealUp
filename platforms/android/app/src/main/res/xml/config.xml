<?xml version='1.0' encoding='utf-8'?>
<widget id="com.momscancode.androidRealUp" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="SplashScreen">
        <param name="android-package" value="org.apache.cordova.splashscreen.SplashScreen" />
        <param name="onload" value="true" />
    </feature>
    <name>RealUp</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="/index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <icon density="hdpi" src="public/assets/images/android/ICONANDROID.png" />
    <icon density="mdpi" src="public/assets/images/android/android_ic_launcher.mdpi.png" />
    <icon density="xhdpi" src="public/assets/images/android/android_ic_launcher.xhdpi.png" />
    <icon density="xxhdpi" src="public/assets/images/android/android_ic_launcher.xxhdpi.png" />
    <icon density="xxxhdpi" src="public/assets/images/android/android_ic_launcher.xxxhdpi.png" />
    <splash density="land-hdpi" src="public/assets/images/screen/android/NEWSPLASHBACKGRND.png" />
    <splash density="land-mdpi" src="public/assets/images/screen/android/AndroidRealUpLogo.mdpi.png" />
    <splash density="land-xhdpi" src="public/assets/images/screen/android/AndroidRealUpLogo.xhdpi.png" />
    <splash density="land-xxhdpi" src="public/assets/images/screen/android/AndroidRealUpLogo.xxhdpi.png" />
    <preference name="loglevel" value="DEBUG" />
</widget>
