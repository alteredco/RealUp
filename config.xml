<?xml version='1.0' encoding='utf-8'?>
<widget id="com.momscancode.androidRealUp" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>RealUp</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <platform name="android">
    <!-- you can use any density that exists in the Android project -->
    <splash src="res/screen/android/AndroidRealUpLogo.hdpi.png" density="land-hdpi"/>
    <splash src="res/screen/android/AndroidRealUpLogo.hdpi.png" density="land-ldpi"/>
    <splash src="res/screen/android/AndroidRealUpLogo.mdpi.png" density="land-mdpi"/>
    <splash src="res/screen/android/AndroidRealUpLogo.xhdpi.png" density="land-xhdpi"/>

    <splash src="res/screen/android/AndroidRealUpLogo.hdpi.png" density="port-hdpi"/>
    <splash src="res/screen/android/AndroidRealUpLogo.hdpi.png" density="port-ldpi"/>
    <splash src="res/screen/android/AndroidRealUpLogo.mdpi.png" density="port-mdpi"/>
    <splash src="res/screen/android/AndroidRealUpLogo.xhdpi.png" density="port-xhdpi"/>
</platform>
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-splashscreen" spec="^5.0.2" />
    <engine name="android" spec="^7.1.1" />
    <engine name="browser" spec="^5.0.4" />
</widget>
